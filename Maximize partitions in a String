class Solution:
    def maxPartitions(self , s):
        # code here
        pos = [0]*26
        for i, e in enumerate(s):
            pos[ord(e)-ord('a')] = i

        running, cnt = -1, 0
        for i, e in enumerate(s):
            running = max(running, pos[ord(e)-ord('a')])
            if running == i:
                cnt += 1
        return cnt
 
