class Solution:
    #User function Template for python3
    
    #Complete this function
    def findFloor(self,A,N,X):
        #Your code here
        m = float("-inf")  
        index = -1  
        
        for i in range(N):
            if A[i] <= X and A[i] > m:
                m = A[i]
                index = i
        
        return index

              
